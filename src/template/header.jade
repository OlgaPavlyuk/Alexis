div(class="page__line page__line--header", id="navbar")
	div.page__cell
		header.header
			a(href="#", class="logo") 
				h1 Alexis
			div.menu-toggle
				span.menu-toggle__lines
			nav.main-menu
				- var menu_list = ["about us", "our focus", "portfolio", "our team", "work steps", "pricing", "testimonials", "blog", "contact"];
				ul.menu__list
					for name in menu_list
						li(class="menu-item")
							a(class="menu__link link", href="\##{name.replace(' ','-')}")= name